{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "id": {
            "type": "string",
            "pattern": "[0-9]+"
        },
        "username": {
            "type": "string"
        },
        "email": {
            "type": "string",
            "pattern": "[a-zA-Z0-9_]+@[a-zA-Z0-9_]+.[a-zA-Z0-9_]+"
        },
        "firstName": {
            "type": "string"
        },
        "lastName": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "id",
        "username",
        "email",
        "firstName",
        "lastName"
    ]
}